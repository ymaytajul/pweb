{
  "version": 2,
  "builds": [
    {
      "src": "**/*.php",
      "use": "@php-runtime/vercel-php"
    }
  ],
  "routes": [
    {
      "src": "/(admin|assets|books|course|fpdf|posts|practice|profile|uploads)/(.*)",
      "dest": "/$1/$2"
    },
    {
      "src": "/.*",
      "dest": "index.php"
    }
  ]
}